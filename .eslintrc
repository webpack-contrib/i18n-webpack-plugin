{
  "extends": "webpack",
  "rules": {
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "no-plusplus": 1,
    "consistent-return": 1,
    "no-multi-assign": 1,
    "no-param-reassign": 1,
    "prefer-destructuring": 1,
    "no-nested-ternary": 1,
    "no-underscore-dangle": [ 2, { "allow": ["__dirname"], "allowAfterThis": true } ],
    "prefer-rest-params": 1
  }
}
